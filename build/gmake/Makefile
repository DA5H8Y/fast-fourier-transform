# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug_x64
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug_x64)
  WaveFileAnalysis_config = debug_x64
endif
ifeq ($(config),release_x64)
  WaveFileAnalysis_config = release_x64
endif

PROJECTS := WaveFileAnalysis

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

WaveFileAnalysis:
ifneq (,$(WaveFileAnalysis_config))
	@echo "==== Building WaveFileAnalysis ($(WaveFileAnalysis_config)) ===="
	@${MAKE} --no-print-directory -C . -f WaveFileAnalysis.make config=$(WaveFileAnalysis_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f WaveFileAnalysis.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug_x64"
	@echo "  release_x64"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   WaveFileAnalysis"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"